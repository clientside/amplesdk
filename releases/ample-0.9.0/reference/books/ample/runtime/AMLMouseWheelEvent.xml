<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="../../../stylesheets/page.xsl"?>
<object name="AMLMouseWheelEvent" extends="runtime:AMLMouseEvent">
	<abstract>
		<locale>
			The MouseWheelEvent interface provides specific contextual information associated with mouse wheel events.
		</locale>
		<locale xml:lang="ru">
			Интерфейс MouseWheelEvent предоставляет контекстную информацию, специфичную для событий манипулятора "мышь".
		</locale>
	</abstract>
	<examples/>
	<members>
		<methods>
			<method name="initMouseWheelEvent" type="Void">
				<arguments>
					<argument name="sEventType" type="String" required="true">
						<description>
							<locale xml:lang="ru">
								Указывает на тип события (Event.type).
							</locale>
						</description>
					</argument>
					<argument name="bCanBubble" type="Boolean" required="true">
						<description>
							<locale xml:lang="ru">
								Указывает, может ли событие распространяться в фазе "bubbling".
							</locale>
						</description>
					</argument>
					<argument name="bCancelable" type="Boolean" required="true">
						<description>
							<locale xml:lang="ru">
								Указывает, возможно ли отменить ответную реакцию браузера по умолчанию на событие.
							</locale>
						</description>
					</argument>
				</arguments>
				<description>
					<locale>
						Initializes attributes of a <link type="object">AMLMouseWheelEvent</link> object.
					</locale>
					<locale xml:lang="ru">
						Метод initMouseWheelEvent используется для инициализации значения объекта MouseEvent.
					</locale>
				</description>
			</method>
		</methods>
		<properties>
			<property name="wheelDelta" type="Number" readonly="true">
				<description>
					<locale>
						The distance the wheel has rotated around the y-axis.
					</locale>
				</description>
			</property>
		</properties>
		<constants />
	</members>
	<remarks>
		<remark>
			<locale>Events of that type:</locale>
      <locale xml:lang="ru">События этого типа:</locale>
      <ul>
        <li><link type="event" href="runtime:AMLElement">mousewheel</link></li>
      </ul>
		</remark>
	</remarks>
	<examples />
</object>